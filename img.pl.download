body {
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif !important;
    font-size: 12px;
    background: white;
    color: black;
    text-align: left;
    vertical-align: middle;
    padding: 0;
    margin: 0;
}

a, .fakelink {
    color: #28642d;
    text-decoration: none;
}

a:hover, .fakelink:hover{
  color:#000;
}

